    <Resource Include="Images\st1.jpeg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\st2.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
